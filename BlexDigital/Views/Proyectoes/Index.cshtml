        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProyectoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProyectoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProyectoId })
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Cotizacion.SolCotizacion.SolCotizacionId)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.ProyectoId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreProyecto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaCreacion)
        </td>
    </tr>
}

</table>
