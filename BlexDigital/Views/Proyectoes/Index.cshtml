@model IEnumerable<BlexDigital.Models.Proyecto>

@{
    ViewBag.Title = "Proyectos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Proyectos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>Acciones</th>
        @*<th>
            @Html.DisplayNameFor(model => model.Cotizacion.SolCotizacion.SolCotizacionId)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.ProyectoId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreProyecto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaCreacion)
        </th>
    </tr>

@foreach (var item in Model) {
